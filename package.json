{
  "name": "@franzgollhammer/eslint-config-standard",
  "version": "0.0.3",
  "description": "ESLint config standard for stylistic rules",
  "private": false,
  "keywords": [
    "eslint",
    "config",
    "style",
    "guide",
    "stylistic"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "release": "npm run lint && npm run build && changeset publish"
  },
  "author": "Franz Gollhammer",
  "license": "MIT",
  "dependencies": {
    "@stylistic/eslint-plugin": "^1.7.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franzgollhammer/eslint-config-standard.git"
  },
  "bugs": {
    "url": "https://github.com/franzgollhammer/eslint-config-standard/issues"
  },
  "homepage": "https://github.com/franzgollhammer/eslint-config-standard#readme"
}
